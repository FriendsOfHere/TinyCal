{
   "name": "小历",
   "version": "1.0.0",
   "identifier": "app.here.tinycal",
   "description": "A simple cal tool",
   "icon": "icon.png",
   "createdBy": "Lifesign",
   "website": "https://github.com/FriendsOfHere/tinycal",
   "appcast": "https://raw.githubusercontent.com/FriendsOfHere/tinycal/master/appcast.xml",
   "script": "index.js",
   "defaultCategory": "quick",
   "displayOn": [
      "menuBar",
   ],
   "preferences": [
      {
         "title": "自定义格式",
         "type": "text",
         "key": "custom-time-format",
         "default": ""
      },
      {
         "title": "农历",
         "type": "popup",
         "key": "lunarFormat",
         "default": 0,
         "options": [
             "关闭",
             "初五",
             "正月初五"
         ]
      },
      {
         "title": "生肖",
         "type": "popup",
         "key": "animalYear",
         "default": 0,
         "options": [
             "关闭",
             "开启",
         ]
      },
   ]
}
